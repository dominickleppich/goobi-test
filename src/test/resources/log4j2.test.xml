<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

	<Appenders>
	    <Console name="stdout">
            <PatternLayout pattern="%-5level %d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %location%n        %msg%n" />
        </Console>
    </Appenders>

	<Loggers>
	    <Root level="FATAL">
	        <AppenderRef ref="stdout" />
	    </Root>
    </Loggers>

</Configuration>
